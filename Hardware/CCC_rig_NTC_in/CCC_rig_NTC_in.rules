
(rules PCB CCC_rig_NTC_in
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 161)
    (layer_rule F.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.5)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.7)
    )
  )
  (rule
    (width 250.0)
    (clearance 200.2)
    (clearance 125.0 (type smd_to_turn_gap))
    (clearance 50.0 (type smd))
    (clearance 200.2 (type "kicad_default"))
    (clearance 200.2 (type "Peltier_out"))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:400_um-Peltier_out" "Via[0-1]_800:400_um" "Peltier_out"
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (via_rule
    "Peltier_out" "Via[0-1]_800:400_um-Peltier_out"
  )
  (class default
    GNDD "NTC_ADC_CH0" "NTC_ADC_CH1" "NTC_ADC_CH2" "NTC_ADC_CH3" "NTC_ADC_CH4" "NTC_ADC_CH5" "NTC_ADC_CH6"
    "NTC_ADC_CH7" +3.3V "3V3_ref_NTC_ADC" +5V "NTC_ADC_CS" "ADC_SPI_MOSI" "ADC_SPI_MISO" "ADC_SPI_CLK"
    "NTC_ADC_CS_00" "NTC_ADC_CS_01" "NTC_ADC_CS_02" "NTC_ADC_CS_03" "NTC_ADC_CS_04" "NTC_ADC_CS_05" "NTC_ADC_CS_06" "NTC_ADC_CS_07"
    "NTC_ADC_CS_08" "NTC_ADC_CS_09" "NTC_ADC_CS_10" "NTC_ADC_CS_11" "NTC_ADC_CS_12" "NTC_ADC_CS_13" "NTC_ADC_CS_14" "NTC_ADC_CS_15"
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    (clearance_class "kicad_default")
    (via_rule "kicad_default")
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "Peltier_out"
    (clearance_class "Peltier_out")
    (via_rule "Peltier_out")
    (rule
      (width 4000.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)