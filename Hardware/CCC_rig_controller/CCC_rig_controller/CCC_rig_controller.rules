    "NTC_ADC_CH4" "NTC_ADC_CH5" "NTC_ADC_CH6" "NTC_ADC_CH7" "3V3_ref_NTC_ADC" +5V "unconnected-(IC1-Pad7)" "unconnected-(IC1-Pad14)"
    "unconnected-(IC1-Pad15)" "ADC_SPI_MISO" "ADC_SPI_MOSI" "ADC_SPI_CLK" "unconnected-(IC1-Pad20)" "NTC_ADC_CS" "Cell_V+" "Iso_5V"
